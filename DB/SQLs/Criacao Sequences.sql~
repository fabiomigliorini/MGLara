CREATE OR REPLACE FUNCTION fntbltituloai()
  RETURNS trigger AS
$BODY$
BEGIN
	IF current_user <> 'mgsis_replicacao' AND current_user <> 'mgsis_yii' THEN
		INSERT INTO tblMovimentoTitulo
			( codTipoMovimentoTitulo 
			, codTitulo 
			, codPortador 
			, codTituloRelacionado
			, Debito
			, Credito 
			, Historico 
			, Transacao 
			, Sistema
			, codTituloAgrupamento 		
			)
		SELECT 
			  tblTipoTitulo.codTipoMovimentoTitulo
			, NEW.codTitulo
			, NEW.codPortador
			, null
			, NEW.Debito
			, NEW.Credito
			, null
			, NEW.Transacao
			, NEW.Sistema
			, NEW.codTituloAgrupamento 		
		FROM tblTipoTitulo 
		WHERE tblTipoTitulo.codTipoTitulo = NEW.codTipoTitulo;
	END IF;
	RETURN NEW;
END;
$BODY$
  LANGUAGE plpgsql VOLATILE
  COST 100;
ALTER FUNCTION fntbltituloai()
  OWNER TO mgsis;


CREATE OR REPLACE FUNCTION fntbltituloau()
  RETURNS trigger AS
$BODY$
DECLARE
	DebitoMov numeric(14,2);
	CreditoMov numeric(14,2);
BEGIN
	IF current_user <> 'mgsis_replicacao' AND current_user <> 'mgsis_yii' THEN
		IF COALESCE(NEW.DEBITO, 0) > COALESCE(OLD.DEBITO, 0) THEN
			DebitoMov = COALESCE(NEW.DEBITO, 0) - COALESCE(OLD.DEBITO, 0);
		END IF;
		IF COALESCE(NEW.DEBITO, 0) < COALESCE(OLD.DEBITO, 0) THEN
			CreditoMov = COALESCE(OLD.DEBITO, 0) - COALESCE(NEW.DEBITO, 0);
		END IF;
		IF COALESCE(NEW.Credito, 0) > COALESCE(OLD.Credito, 0) THEN
			CreditoMov = COALESCE(NEW.Credito, 0) - COALESCE(OLD.Credito, 0);
		END IF;
		IF COALESCE(NEW.Credito, 0) < COALESCE(OLD.Credito, 0) THEN
			DebitoMov = COALESCE(OLD.Credito, 0) - COALESCE(NEW.Credito, 0);
		END IF;
		IF COALESCE(DebitoMov, 0) <> 0 OR COALESCE(CreditoMov, 0) <> 0 THEN
			INSERT INTO tblMovimentoTitulo
				( codTipoMovimentoTitulo 
				, codTitulo 
				, codPortador 
				, codTituloRelacionado
				, Debito
				, Credito 
				, Historico 
				, Transacao 
				, Sistema 		
				)
			SELECT 
				  200
				, NEW.codTitulo
				, NEW.codPortador
				, null
				, DebitoMov
				, CreditoMov
				, null
				, NOW()
				, NOW()
			FROM tblTipoTitulo 
			WHERE tblTipoTitulo.codTipoTitulo = NEW.codTipoTitulo;
		END IF;
	END IF;
	RETURN NEW;
END;
$BODY$
  LANGUAGE plpgsql VOLATILE
  COST 100;
ALTER FUNCTION fntbltituloau()
  OWNER TO mgsis;



DROP FUNCTION fnpegaproximocodigo(text);

DROP TRIGGER tbltitulobi ON tbltitulo;
DROP FUNCTION fntbltitulobi();

